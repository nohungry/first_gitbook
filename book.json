{
    "title": "Sample gitbook",
    "description": "sample book created by gitbook",
    "author": "nohungry <tzuyang1991@gmail.com>",
    "language": "en",
    "plugins": ["livereload",
                "highlight",
                "-search", "search-pro"],
    "pluginsConfig": {
        "highlight": {
            "theme": "monokai"
        }
    },
    "structure": {
        "readme": "README.md",
        "summary": "SUMMARY.md"
    },
    "styles": {
        "website": "styles/website.css"
    },
    "generator": "site",
    "links": {
        "sharing": {
            "all": false,
            "facebook": false,
            "google": false,
            "twitter": false
        }
    }
}
